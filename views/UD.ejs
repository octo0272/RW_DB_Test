<!DOCTYPE html>
<html lang="ko">
<head>
  <meta charset="UTF-8">
  <title>User list</title>
</head>
<body>
  <table>
    <tr>
      <th>이름</th>
      <th>주소</th>
      <th>수정</th>
      <th>삭제</th>
    </tr>
    <% sample_table.forEach(function (value) { %>
    <tr>
      <td><%= value.name %></td>
      <td><%= value.address %></td>
      <td><a href="/edit/<%= value.id %>">수정</a></td>
      <td><a href="/delete/<%= value.id %>">삭제</a></td>
    </tr>
    <% }); %>
  </table>
</body>
</html>
